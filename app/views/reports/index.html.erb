<p id="notice"><%= notice %></p>

<h1>Reports</h1>

<table>
  <thead>
    <tr>
      <th>Symp</th>
      <th>Analysis</th>
      <th>Presc</th>
      <th>Patient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reports.each do |report| %>
      <tr>
        <td><%= report.symp %></td>
        <td><%= report.analysis %></td>
        <td><%= report.presc %></td>
        <td><%= report.patient %></td>
       
      </tr>
    <% end %>
  </tbody>
</table>

<br>


